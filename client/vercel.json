{
  "buildCommand": "NODE_OPTIONS='--max-old-space-size=4096' npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "its-attendance-system.vercel.app/api/$1"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
